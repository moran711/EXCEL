{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "linebreak-style":"off",
        "indent": "off",
        "operator-linebreak": "off",
        "no-case-declarations": "off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}